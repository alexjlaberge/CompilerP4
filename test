class Cow extends Animal
{
	int b;
	void setNum(int p)
	{
		a = p;
		b = p;
	}
	int getNum()
	{
		return a;
		return b;
	}
}

class Animal
{
	int a;
	int q;
	int c;
	int k;
	int getPie()
	{
		return k;
	}
	void setPie(int a)
	{
		this.a = a;
	}
}

int main()
{
	Cow[] a;
	int i;
	a = NewArray(5,Cow);
	for(i = 0; i < 5; i = i + 1)
	{
		a[i] = New(Cow);
	}
	for(i = 0; i < 5; i = i + 1)
	{
		a[i].setPie(i);
	}
	for(i = 0; i < 5; i = i + 1)
	{
		Print(a[i].getPie());
	}


}

