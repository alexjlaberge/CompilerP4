	//Print(betsy.geneticSpots());

	c = a * b + c;
	boscoe = NewArray(10, int);
	boscoe[1] = boscoe[1] + v(a,b,c);
	return v(a,b,c);
}

int v(int a, int b, int c)
{
	int[] arr;
	
	int d;
	int e;
	int f;
	boscoe = NewArray(3, int);
	arr = NewArray(3,int);
	arr[0] = arr[1] + arr[2] + arr[0];
	f = 2 + e;
	e = 3 + 4 + d + c;
	if(a == 2)
	{
		int y;
		int z;
		y = 2;
		z = 2;
		return y;
		return z;
	}
	return c;
}

