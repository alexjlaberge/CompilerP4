int[] boscoe;
Cow sasha;
class Cow
{
	bool hasSpots;
	int numSpots;
	Cow parent;
	void initCow()
	{
		hasSpots = true;
	}
	void setSpots(int a)
	{
		numSpots = a;
	}
	int getSpots()
	{
		return numSpots * numSpots;
	}
	bool geneticSpots()
	{
		return parent.hasSpots;
		return sasha.hasSpots;
	}
}

int main()
{
	int a;
